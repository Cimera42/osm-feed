    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "name": "Run feed",
            "request": "launch",
            "runtimeExecutable": "yarn",
            "runtimeArgs": ["start", "feed"],
            "skipFiles": ["<node_internals>/**"]
        },
        {
            "type": "node",
            "name": "Generate bounds",
            "request": "launch",
            "runtimeExecutable": "yarn",
            "runtimeArgs": ["start", "bounds", "Australia"],
            "skipFiles": ["<node_internals>/**"]
        },
        {
            "type": "node",
            "name": "Lint",
            "request": "launch",
            "runtimeExecutable": "yarn",
            "runtimeArgs": ["eslint", "src"],
            "skipFiles": ["<node_internals>/**"]
        }
    ]
}
